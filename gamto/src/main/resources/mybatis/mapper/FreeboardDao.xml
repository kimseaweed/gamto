<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ex.gamto.freeboard.dao.IFreeboardDao">
	<!-- 자유게시판 리스트 -->
	<select id="listDao" resultType="com.ex.gamto.freeboard.dto.FreeboardDto">
		select*from freeboard order by f_seq_number desc
	</select>
	<select id="viewDao" resultType="com.ex.gamto.freeboard.dto.FreeboardDto">
		select*from freeboard where f_seq_number=#{0}
	</select>
	<insert id="writeDao">
		insert into freeboard values(free_board_seq.nextval,#{item1},#{item2},#{item3})
	</insert>
	<delete id="deleteDao">
		delete freeboard where f_seq_number=#{newId}
	</delete>
	



</mapper>